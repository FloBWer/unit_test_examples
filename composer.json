{
    "name": "symfony/phpunit-example",
    "description": "Symfony Projekt f√ºr PHPUnit Lernsession",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "symfony/framework-bundle": "^6.0",
        "symfony/console": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "symfony/test-pack": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true
        },
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}

